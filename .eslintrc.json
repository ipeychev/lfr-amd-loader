{
    "extends": "liferay",
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "module"
    },
    "env": {
        "es6": false
    },
    "rules": {
        "no-var": "off",
        "object-curly-spacing": "off",
        "prefer-spread": "off",
        "quote-props": ["error", "as-needed"],
        "quotes": ["error", "single", {
            "avoidEscape": true
        }]
    }
}
